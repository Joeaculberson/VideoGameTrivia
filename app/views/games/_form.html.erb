<%= simple_form_for(@game) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :opponent_user_email, :url => autocomplete_user_email_games_path, :as => :autocomplete %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-info btn-lg btn-block' %>
  </div>
<% end %>
